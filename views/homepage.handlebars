<div class="left-col">
	<a href="/">
		<div class="logo">
			Sample Code
		</div>
	</a>
	<div class="list-title">
		Frameworks
	</div>
	<div class="col-item-list">
		{{#each frameworks}}
			<a href="/framework/{{name}}">
				<div class="row-item">{{name}}</div>
			</a>	
		{{/each}}
	</div>
	<a href="/submit">
		<div class="col-item submit-sample">
			Submit Sample
		</div>
	</a>
</div>

<div class="content-col">
	<div class="recent-samples">
		<div class="header">
			<strong>Last 10 samples submitted</strong>
		</div>
		{{#each recentSamples}}
			<div class="recent-sample">
				<a href="/sample/{{title}}"><p class="title">{{title}}</p></a>
				<p class="description">{{description}}</p>
			</div>
		{{/each}}
	</div>
</div>
